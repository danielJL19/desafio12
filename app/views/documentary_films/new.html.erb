<h1>DocumentaryFilms#new</h1>
<h2>Formulario</h2>
<%= form_with model: @documentaryFilm do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :synopsis, "Sinopsis" %>
    <%= form.text_area :synopsis %>
  </div>
  <div>
    <%= form.label :director %>
    <%= form.text_field :director %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>

